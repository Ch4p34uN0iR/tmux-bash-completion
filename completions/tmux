# tmux(1) completion                                       -*- shell-script -*-


_tmux()
{
    local cur prev words cword
    _init_completion || return

    if [[ $cword -eq 1 ]]; then
        COMPREPLY=( $( compgen -W "$(tmux list-commands -F '#{command_list_name}')" -- "$cur" ) )
    fi

} &&
complete -F _tmux tmux

# ex: filetype=sh
